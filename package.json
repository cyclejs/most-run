{
  "name": "@cycle/most-run",
  "version": "1.0.0",
  "description": "Cycle.run() for most.js",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/",
    "mocha": "mocha --compilers js:babel-register test",
    "test": "npm run lint && npm run mocha",
    "prelib": "rm -rf lib/ && mkdirp lib/",
    "lib": "babel src -d lib",
    "predist": "rimraf dist/ && mkdirp dist/ && npm run lib",
    "dist": "browserify -t browserify-shim -t babelify --exclude most src/index.js -o dist/cycle-most-run.js",
    "postdist": "uglifyjs dist/cycle-most-run.js -o dist/cycle-most-run.min.js",
    "start": "npm install && npm prune",
    "prepublish": "npm run lib",
    "preversion": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyclejs/most-run.git"
  },
  "keywords": [
    "most.js",
    "Cycle.js",
    "Diversity"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com> (github.com/TylorS)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyclejs/most-run/issues"
  },
  "homepage": "https://github.com/cyclejs/most-run#readme",
  "dependencies": {
    "@cycle/base": "^2.1.0",
    "@cycle/most-adapter": "^1.0.0"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "uglifyjs": "^2.4.10"
  }
}
